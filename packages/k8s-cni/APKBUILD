depends=""
makedepends=""
install=""
subpackages=""
source="https://github.com/containernetworking/plugins/releases/download/${pkgver}/cni-plugins-arm-${pkgver}.tgz"
builddir="$srcdir/"
options="!check"

build() {
        return 0
}

package() {
        mkdir -p "$pkgdir"/data/k8s/cni/bin
        tar -xzf "$srcdir"/cni-plugins-arm-"$pkgver".tgz -C "$pkgdir"/data/k8s/cni/bin
        return 0
}
